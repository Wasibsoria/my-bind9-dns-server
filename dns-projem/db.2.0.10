;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.seyit.local. root.seyit.local. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns1.seyit.local.
15	IN	PTR	ns1.seyit.local.
15	IN	PTR	www.seyit.local.
15	IN	PTR	ftp.seyit.local.
15	IN	PTR	mail.seyit.local.

